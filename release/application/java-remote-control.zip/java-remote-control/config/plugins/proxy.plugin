<plugin class="org.one.stone.soup.xapp.plugin.proxy.ProxyPlugin" name="Proxy Settings">
	<form name="Proxy Form" cellPadding="5" rowPadding="2">
		<tr>
			<td colspan="4">
				<h size="16" face="Arial" style="BOLD">
<![CDATA[Proxy Settings]]>
				</h>
			</td>
		</tr>
		<tr>
			<td colspan="4">
				<separator width="300"/>
			</td>
		</tr>
		<tr>
			<td>
<![CDATA[Enable Proxy]]>
			</td>
			<td colspan="3">
				<input type="checkbox" name="useProxy"/>
			</td>
		</tr>
		<tr>
			<td>
<![CDATA[Host]]>
			</td>
			<td>
				<input type="text" name="host" size="20"/>
			</td>
			<td>
<![CDATA[Port]]>
			</td>
			<td>
				<input type="text" name="port" format="Integer" size="4" value="80"/>
			</td>
		</tr>
		<tr>
			<td colspan="4">
				<input type="button" name="setProxy" value="Apply"/>
			</td>
		</tr>
	</form>
</plugin>